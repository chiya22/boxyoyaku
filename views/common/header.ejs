<div class="header">
    <div class="header_main">
        <div id="top_logo" class="header_left">
            <img class="header_img" src="/static/images/building_logo.svg">
        </div>
        <div class="header_right">
            <% if (locals.user) { %>
            <a class="header_menu header_profile" href="/users/updinfo">
                <img class="header_icon_img" src="/static/images/profile.svg">
            </a>
            <form class="header_menu header_logout" method="post" action="/logout">
                <input type="image" class="header_logout_icon_img" src="/static/images/logout.svg" />
            </form>
            <% } %>
        </div>
    </div>
</div>